         .x-small{
             font-size:x-small;
         }

         .small {
             font-size: small;
         }

         .hand {
             cursor: pointer;
         }

         .padding-left-3 {
             padding-left: 3px !important;
         }
         .rightbar-status-spot-green {
             font-size:0.1em;
             padding-top:3px;
             padding-right:8px;
             color: rgb(124, 200, 63);
         }
         .rightbar-status-spot-grey {
             font-size:0.1em;
             padding-top:3px;
             padding-right:8px;
             color: rgb(192, 192, 192);
         }
         .rightbar-status-spot-red {
             font-size:0.1em;
             padding-top:3px;
             padding-right:8px;
             color: rgb(228, 120, 149);
         }

         .rightbar-status-spot-eye {
             font-size:0.1em;
             color:rgb(184, 184, 184);
             margin-left:5px;
         }

         .rightbar-status-group-eye {
             color: #666565;
         }
         .rightbar-status-group-eye-slash {
             color: #838282;
             text-decoration:line-through;
         }


         [ng-cloak] {
             display: none;
         }

         .topNegative1000 {
             top: -1000px !important;
         }

         .topZero {
             top: 0 !important;
         }

         .mainview-animation {
             position: relative;
         }

         .mainview-animation.ng-enter {
             -webkit-transition: .3s linear all; /* Safari/Chrome */
             -moz-transition: .3s linear all; /* Firefox */
             -o-transition: .3s linear all; /* Opera */
             transition: .3s linear all; /* IE10+ and Future Browsers */
         }

         /**
          * Pre animation -> enter
          */
         .mainview-animation.ng-enter {
             /* The animation preparation code */
             opacity: 0;
         }

         /**
          * Post animation -> enter
          */
         .mainview-animation.ng-enter.ng-enter-active {
             /* The animation code itself */
             opacity: 1;
         }

         .angular-google-map-container {
             height: 300px;
         }

         .navbar.navbar-default.ng-hide {
             display: none;
         }

         #page-rightbar .jspHorizontalBar {
             /*display: none !important;*/
         }

         .fc-grid .fc-day-number {
             padding: 5px 6px;
         }

         .logout-lable {
             text-align:right;
             padding-right:20px;
             font-weight:bold;
         }

         .logout-lable:hover {
             background-color: rgb(236, 240, 227);
             cursor: pointer;
         }

        </style>
